<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="cn.pursol.test.bo.Article" table="ARTICLE">
<id name="cid" type="java.lang.Integer" column="cid">  <generator class="native" /> </id>
<property  name="title" type="java.lang.String" column="title"/>
<property  name="author" type="java.lang.String" column="author"/>
<property  name="createtime" type="java.lang.String" column="createtime"/>
<property  name="text" type="java.lang.String" column="text"/>
<property  name="userid" type="java.lang.Integer" column="userid"/>
<property  name="state" type="java.lang.String" column="state"/>
<property  name="classifyid" type="java.lang.Integer" column="classifyid"/>
</class>
</hibernate-mapping>