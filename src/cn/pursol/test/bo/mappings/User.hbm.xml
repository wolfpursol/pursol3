<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="cn.pursol.test.bo.User" table="USER">
<id name="id" type="java.lang.Integer" column="id">  <generator class="native" /> </id>
<property  name="name" type="java.lang.String" column="name"/>
<property  name="age" type="java.lang.Integer" column="age"/>
<property  name="createtime" type="java.lang.String" column="createtime"/>
<property  name="state" type="java.lang.String" column="state"/>
</class>
</hibernate-mapping>